<div class="row">
<div class="span12">
<h1>Login</h1><hr>
</div>
</div>

<div class="row">
<div class="span 4 offset4">
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => { :class => "well form-vertical"}) do |f| %>
  <label><%= f.label :email %></label>
  <%= f.text_field :email %>

  <label><%= f.label :password %></label>
  <%= f.password_field :password %>

  <% if devise_mapping.rememberable? -%>
    <label class="checkbox"><%= f.check_box :remember_me %> <%= f.label :remember_me %></label>
  <% end -%>

  <p align="right"><%= f.submit "Log in", :class => "btn"  %></p>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>